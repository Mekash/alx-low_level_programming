#include<stdio.h>
#include<stdlib.h>
#include<time.h>
  
/* Driver program */

int main(void)

{
    /*This program will create different sequence of 
     random numbers on every program run Use current time as seed for random generator
    */
    int n = 0;
	srand(time(0));
	n = rand() - RAND_MAX/2;
	
	if (n>0)
	{
	    printf("positive");
	}
	else if (n<0)
	{
	    printf("negative");
	}
	else 
	{
	    printf("zero");
	}

    return 0;
}

